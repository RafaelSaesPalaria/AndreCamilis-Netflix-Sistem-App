<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/black"
    tools:context=".View.TelaPerfil">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/fotonetpronto"
        android:scaleX="1.3"
        android:scaleY="1.3"
        android:alpha="0.4"/>



    <!--IMAGEM 1-->
    <ImageView
        android:id="@+id/imgAzul"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginStart="@dimen/_80sdp"
        android:layout_marginTop="@dimen/_280sdp"
        android:clickable="true"
        android:src="@drawable/foto1"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />



    <!--IMAGEM 2-->
    <ImageView
        android:id="@+id/imgRosa"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginStart="@dimen/_141sdp"
        android:layout_marginTop="@dimen/_280sdp"
        android:clickable="true"
        android:src="@drawable/foto2"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />



    <!--IMAGEM 3-->
    <ImageView
        android:id="@+id/imgVerde"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginStart="@dimen/_199sdp"
        android:layout_marginTop="@dimen/_280sdp"
        android:clickable="true"
        android:src="@drawable/foto3"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />




    <!--IMAGEM 4-->
    <ImageView
        android:id="@+id/imgVermelho"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginStart="@dimen/_109sdp"
        android:layout_marginTop="@dimen/_350sdp"
        android:clickable="true"
        android:src="@drawable/foto4"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />




    <!--IMAGEM 5-->
    <ImageView
        android:id="@+id/imgAmarelo"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginStart="@dimen/_169sdp"
        android:layout_marginTop="@dimen/_350sdp"
        android:clickable="true"
        android:src="@drawable/foto5"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />




    <TextView
        android:layout_width="@dimen/_191sdp"
        android:layout_height="@dimen/_16sdp"
        android:text="@string/infoTelaPerfil"
        android:textColor="@color/white"
        android:textSize="@dimen/_13sdp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="@dimen/_69sdp"
        android:layout_marginTop="@dimen/_250sdp" />


</RelativeLayout>